{
  "name": "generator-ajsweb",
  "version": "1.7.5",
  "description": "Yeoman Gulp generator for AngularJS Web Application",
  "homepage": "https://github.com/yadickson/generator-ajsweb",
  "author": {
    "name": "Yadickson Soto",
    "email": "yadickson@gmail.com",
    "url": "https://github.com/yadickson"
  },
  "files": [
    "generators/app",
    "generators/route",
    "generators/controller",
    "generators/view",
    "generators/directive",
    "generators/filter",
    "generators/service",
    "generators/provider",
    "generators/factory",
    "generators/value",
    "generators/constant",
    "generators/decorator",
