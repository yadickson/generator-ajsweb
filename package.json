    "file-system": "^2.2.2",
    "generator-ajsbase": "~1.7.1",
    "package-json-utils": "^1.1.0",
    "string-template": "^1.0.0",
    "stringjector": "^1.0.1",
    "upper-case": "^1.1.3",
    "uppercamelcase": "^3.0.0",
    "yeoman-generator": "^2.0.1",
    "yosay": "^3.0.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "mkdirp": "^0.5.1",
